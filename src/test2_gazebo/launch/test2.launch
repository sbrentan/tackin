<launch>
  <!-- We resume the logic in empty_world.launch, changing only the name of the world to be launched -->

  <rosparam file="$(find test2_gazebo)/config/gazebo_ros_control_params.yaml" command="load"/>

  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find test2_gazebo)/worlds/test2.world"/>
    <arg name="verbose" value="true" />
    <!--arg name="paused" value="true" /-->
    <!-- more default parameters can be changed here -->
  </include>

  <!--include ns="test2" file="$(find test2_gazebo)/launch/spawn_robot.launch"/-->

</launch>
