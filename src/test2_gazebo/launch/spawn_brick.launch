<launch>
  <!-- Convert xacro and put on parameter server -->
  <param name="robot_description" command="$(find xacro)/xacro $(find test2_gazebo)/models/bricks/$(arg model)/model.sdf" />

  <!-- Spawn a robot into Gazebo -->
  <node name="spawn_sdf" pkg="gazebo_ros" type="spawn_model" args=" -sdf -param robot_description -model $(arg name)
      -x $(arg x) -y $(arg y) -z 0" />

</launch>