<launch>
  <!-- Convert xacro and put on parameter server -->
  <param name="robot_description" command="$(find xacro)/xacro $(find test2_description)/urdf/grasper.urdf" />

  <!--node pkg="rosservice" type="rosservice" name="delete_robot" args="call wait /gazebo/delete_model test2" /-->

  <!-- Spawn a robot into Gazebo -->
  <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" args="-param robot_description -urdf -x 0 -y 0 -z 1 -model grasper -J shoulder_lift_joint -0.785 -J elbow_joint 0.785 -J wrist_1_joint -1.57 -J wrist_2_joint -1.57 " />

</launch>